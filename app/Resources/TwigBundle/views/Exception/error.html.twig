    <link rel="stylesheet" href="{{ asset('bundles/africanuser/css/style.css') }}"/>

{% endblock css %}

	<style>
	.error-page{background:#50d8af;position:absolute;height: 100%;
	   width: 100%;}
	.error-page .error-inner{height:100%;display:table;text-align: center;
	   width: 100%;}
	.error-page .table-inner{display:table-cell;height:100%;vertical-align:middle;}
	.error-page h1{font-size:150px;color:#fb503b;margin:0;}
	.error-page p{font-size:30px;}
	 @media (max-width: 768px){.error-page p{font-size:21px;}.error-page h1{font-size:100px;}}

	</style>

<body id="body">
  <!--==========================
    Top Bar
  ============================-->
  <section id="topbar" class="d-none d-lg-block">
    <div class="container clearfix">
      <div class="contact-info float-left">
        <i class="fa fa-envelope-o"></i> <a href="mailto:sales@brainwit.co.in">sales@brainwit.co.in</a>
        <i class="fa fa-phone"></i> +91 8320349417
      </div>
    </div>
  </section>

  <!--==========================
    Header
  ============================-->
  <header id="header">
  	<div class="container">
      <div id="logo" class="pull-left">
          <a href="/"><img src="img/logo.png"></a>
      </div>
    </div>
  </header><!-- #header -->

  <!--==========================
    Intro Section
  ============================-->
    <section id="intro">
    	<div class="error-page">
         <div class="error-inner">
              <div class="table-inner">
                    <h1>404</h1>
                    <p>Error - Page Not Found!</p>
              </div>
         </div>
	   </div>
    </section><!-- #intro -->

  <!--==========================
    Footer
  ============================-->
  <footer id="footer">
    <div class="container">
      <div class="copyright">
        &copy; Copyright 2017 - {{ "now"|date("Y") }} <strong>Brainwit System</strong>. All Rights Reserved
      </div>
      <div class="credits"></div>
    </div>
  </footer><!-- #footer -->

{% block javascript %}

<!-- JS -->
<script src="{{ asset('bundles/africanuser/lib/jquery/jquery.min.js') }}"></script>
